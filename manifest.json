{
  "manifest_version": 2,
  "name": "Ducky MDN",
  "description": "Used DuckDuckGo's \"I'm feeling ducky\" to search for MDN (developer.mozilla.org). Useful for developers.",
  "version": "1.1.0",
  "icons": {
    "16": "images/icon-16.png",
    "19": "images/icon-19.png",
    "32": "images/icon-32.png",
    "38": "images/icon-38.png",
    "48": "images/icon-48.png",
    "64": "images/icon-64.png",
    "96": "images/icon-96.png"
  },
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Ducky MDN",
      "search_url": "https://duckduckgo.com/?q=%21%20site%3Adeveloper.mozilla.org%20{searchTerms}",
      "keyword": "mdn",
      "favicon_url": "https://developer.mozilla.org/favicon.ico",
      "is_default": false
    }
  }
}